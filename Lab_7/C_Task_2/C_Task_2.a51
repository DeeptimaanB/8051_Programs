ORG 0000H
    REPEAT: MOV TMOD,#10H
	MOV R0,#4
    BACK:MOV TL1,#00H
    MOV TH1,#00H
	CLR P1.7
    SETB TR1
AGAIN:JNB TF1,AGAIN
    CLR TR1
    CLR TF1
    DJNZ R0,BACK
	MOV TMOD,#01H
	MOV R1,#11
    BACK1:MOV TL0,#00H
    MOV TH0,#00H
	SETB P1.7
    SETB TR0
    AGAIN1:JNB TF0,AGAIN1
    CLR TR0
    CLR TF0
    DJNZ R1,BACK1
    SJMP REPEAT
    END ;Deeptimaan Banerjee
